        </Style>

        <Style x:Key="AnilibriaSidebarList" TargetType="ListView">
            <Setter Property="ItemContainerStyle" Value="{StaticResource SidebarListViewItem}" />
        </Style>
        <Style x:Key="DarkAnilibriaSidebarList" TargetType="ListView">
            <Setter Property="ItemContainerStyle" Value="{StaticResource DarkSidebarListViewItem}" />
        </Style>

    </Application.Resources>

</Application>
